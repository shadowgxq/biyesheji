.clearfix:after {
  content: "";
  display: table;
  clear: both;
}
.content {
  width: 100vw;
  height: 100vh;
  background-color: #f5f5f5;
}
.content .content_item {
  padding: 30rpx;
  background-color: #fff;
  border-radius: 16rpx;
  margin: 16rpx;
}
.content .content_item .item_content {
  display: flex;
}
.content .content_item .item_content .item_img {
  width: 160rpx;
}
.content .content_item .item_content .item_img image {
  width: 100%;
  height: 160rpx;
}
