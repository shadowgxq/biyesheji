/* pages/my/my.wxss */
/* login的书写 */
.my button {
  padding:0;
  width: 100%!important;
}
.my {
  height: 100vh;
  background-color: #f5f5f5;
}
.login-on {
  background-color: #eee;
  display: flex;
  height: 200rpx;
  justify-content: center;
  align-items: center;
}

.login-no {
  display: flex;
  background: url(data:image/jpg;base64,);
  height: 300rpx;
  padding: 20rpx 30rpx;
  
}
.login-no .img{
  margin-top: 20rpx;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  overflow: hidden;
  border: 2px solid #fff;
}
.login-no .img image {
  width: 120rpx;
  height: 120rpx;
}
.login-no .user {
  flex: 1;
  padding-left: 30rpx;
  font-size: 36rpx;
  margin-top: 30rpx;
  color: #fff;
}
.login-no .dengji {
  font-size: 26rpx;
  color: #fff;
  padding-top: 10rpx;
}
.login-no .block{
  flex: 2;
}
.login-no .setting{
  width: 50rpx;
  color: #fff;
  font-size: 50rpx;
}
 .bar{
  display: flex;
  justify-content: space-around;
  margin-top: -100rpx;
  color: #fff;
  font-weight: 600;
  font-size: 26rpx;
  padding-bottom: 30rpx;
}
.bar .bar-item{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.member-box{
  width: 690rpx;
  height: 250rpx;
  background-color: #fff;
  border-top: 20rpx solid rgba(100,100,100,.1);
  padding: 30rpx 30rpx;
}
.member-box .order{}
.member-box .order .order_top{
  display: flex;
  justify-content: space-between;
}
.member-box .order .order_top view:nth-child(1) {
  font-weight: 600;
}
.member-box .order .order_top view:nth-child(2) {
  font-size: 20rpx;
  color: #333;
}
.member-box .order .order_button{
  display: flex;
  margin-top: 60rpx;
}
.member-box .order .order_button .order_item{
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 24rpx;
}
.member-box .order .order_button .order_item .iconfont{
  font-size: 50rpx;
  color: rgb(211, 58, 58);
  margin-bottom: 20rpx;
}
.tuijian{
  width: 100%;
  text-align: center;
  font-weight: 800;
  padding: 20rpx 0rpx;
}
.huiyuan {
  margin-top: -10rpx;
  width: 94%;
  background-color: #3bba63;
  padding: 10rpx;
  color: white;
  margin: 0 auto;
  border-radius: 10rpx;
  display: flex;
  margin-bottom: 20rpx;
  font-size: 20rpx;
  font-weight: 500;
  justify-content: space-between;
  align-items: center;
  opacity: .8;
}
.huiyuan .left image {
  width: 24rpx;
  height: 24rpx;
  padding-right: 20rpx;
}