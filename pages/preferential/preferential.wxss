/* pages/preferential/preferential.wxss */
page {
  height: 100%;
}
.preferentail {
  height: 100%;
  background-color: #f5f5f5;
}
.top {
  background-color: #FFF8E6;
  font-size: 32rpx;
  padding:10rpx;
  font-family: "宋体";
  font-weight: bold;
}
.content {
  text-align: center;
}
.content image {
  margin-top: 30rpx;
  width: 680rpx;
  height: 220rpx;
}
.tab {
  display: flex;
  background-color: #fff;
  text-align: center;
  color: #333;
}
.tab view {
  flex: 1;
  padding: 30rpx 0;
  margin: 0 50rpx;
}
.tab .current {
  color: #f1c237;
  border-bottom: 2rpx solid #f1c237;
}
